{
    "dataset": {
        "name" : "m_gme",
        "path": "datasets/gme_d.csv",
        "column_price": "close",
        "time_column": "start",
        "eval_proportion": 0.16,
        "test_proportion": 0.2
    },

    "model": {
        "epochs_for_Q_Learning_fit": "auto",
        "batch_size_for_learning": 300,
        "l2_penalty": 2e-4,
        "dropout_level": 0.1,
        "scale_NN": 4
    },

    "rewards": ["LR", "SR", "ALR", "POWC"],

    "window_size": 60,
    
    "episode_length": 1000,
    "frequency_q_learning": 250,
    "frequency_target_exchange": 4,  
    "batch_size_replay_sampling": 1500,
    "Q_learning_iterations": 250,
    "discount_factor_Q_learning": 0.9,
    "random_gamma": false,
    "long_positions_only": false
}
